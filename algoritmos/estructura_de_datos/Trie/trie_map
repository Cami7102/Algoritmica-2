#include <bits/stdc++.h>
#define input freopen("in.txt", "r", stdin)
#define output freopen("out.txt", "w", stdout)
using namespace std;

int numberOfNodes = 0;

struct node{
    map<char, node*> mapa;
    bool isWord;                
} *trie;

void init(){
    trie = new node();
    trie->isWord = false;
    numberOfNodes++;
}

void insertWord(string word){
    node *currentNode = trie;  
    for (int i = 0; i < word.length(); i++){
        
        if(currentNode->mapa.find(word[i]) == currentNode->mapa.end())
        {
            currentNode->mapa[word[i]] = new node();
            numberOfNodes++;
        }
        currentNode = currentNode->mapa[word[i]];
    }
    currentNode->isWord=true;
    cout<<"Se insertÃ³ la palabra "<<word<<endl;
}

int main() {

    // Inicializar Trie
    init(); 
    string word = "alto";
    insertWord(word);
    string word = "auto";
    insertWord(word);
    word = "automovil";   
    insertWord(word);
    cout<<numberOfNodes++<<endl;
    return 0;
}
